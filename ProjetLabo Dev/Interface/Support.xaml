        <Button x:Name="btnEnvoyerMessage" Content="Envoyer" HorizontalAlignment="Left" Margin="352,136,0,0" VerticalAlignment="Top" Width="75" Click="btnEnvoyerMessage_Click"/>

    </Grid>

</Page>
