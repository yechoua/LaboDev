<Window x:Class="ProjetLabo_Dev.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="500" Width="880">
    <DockPanel>
        <Grid>
            <Button x:Name="btnGestion" Content="Gestion" HorizontalAlignment="Left" Margin="0,51,0,0" VerticalAlignment="Top" Width="75" Click="btnGestion_Click" />
            <Button x:Name="btnParametre" Content="Paramètre" HorizontalAlignment="Left" Margin="0,90,0,0" VerticalAlignment="Top" Width="75" Click="BtnParametre_OnClick" Visibility="Hidden" />
            <Button x:Name="btnPlanning" Content="Planning" HorizontalAlignment="Left" Margin="0,125,0,0" VerticalAlignment="Top" Width="75" Click="btnPlanning_Click" />
            <Button x:Name="btnSupport" Content="Support" HorizontalAlignment="Left" Margin="0,164,0,0" VerticalAlignment="Top" Width="75" Click="btnSupport_Click" Visibility="Hidden" />
            <Button x:Name="btnClients" Content="Clients" HorizontalAlignment="Left" Margin="0,10,0,0" VerticalAlignment="Top" Width="75" Click="btnClients_Click" />
        </Grid>
        <Grid x:Name="_mainFrame" />
        <!--<TextBox x:Name="TesTextBoxCoonection" Height="34" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Margin="0,100,0,0"/>-->
    </DockPanel>
</Window>
